{
  "name": "pmm-app",
  "version": "2.10.0",
  "description": "PMM UI",
  "author": "Percona",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/percona/grafana-dashboards.git"
  },
  "main": "module.js",
  "scripts": {
    "build": "npm run lint:tests && grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "test:update": "grafana-toolkit plugin:test --updateSnapshot",
    "coverage": "grafana-toolkit plugin:test --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "lint": "npm run lint:dev && npm run lint:tests",
    "lint:dev": "eslint --ext .tsx,.ts --fix src/",
    "lint:tests": "eslint --ext .js --fix tests/",
    "jest": "jest",
    "e2e": "docker run --net=host -v $PWD:/tests -v $PWD/node_modules:/node_modules --env-file env.generated.list codeception/codeceptjs:2.6.1 codeceptjs run-multiple parallel --all --debug --steps --reporter mocha-multi -c pr.codecept.js --grep '(?=.*)^(?!.*@not-ui-pipeline)^(?!.*@not-pr-pipeline)'",
    "e2e:local": "docker run --net=host --name codecept -v $PWD:/tests -v $PWD/node_modules:/node_modules codeception/codeceptjs:2.6.1 codeceptjs run-multiple parallel --all --debug --steps -c pr-local.codecept.js --grep '(?=.*)^(?!.*@not-ui-pipeline)^(?!.*@not-pr-pipeline)'",
    "e2e:qan": "docker run --net=host -v $PWD:/tests codeception/codeceptjs:2.6.1 codeceptjs run --steps  -c pr.codecept.js --grep '@new-qan' ",
    "e2e:current": "docker run --net=host -v $PWD:/tests codeception/codeceptjs:2.6.1 codeceptjs run --steps  -c pr.codecept.js --grep '@current' ",
    "codecov": "codecov"
  },
  "bin": {
    "toolkit": "node_modules/@grafana/toolkit/bin/grafana-toolkit.dist.js"
  },
  "keywords": [
    "percona",
    "qan",
    "pmm",
    "grafana"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@grafana/data": "7.1.3",
    "@grafana/runtime": "7.1.3",
    "@grafana/ui": "7.1.3",
    "@percona/platform-core": "^0.4.0",
    "@emotion/core": "^10.0.35",
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "d3": "^5.16.0",
    "emotion": "10.0.27",
    "final-form": "^4.20.1",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "moment": "^2.26.0",
    "numeral": "^2.0.6",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-final-form": "^6.5.0",
    "react-highlight.js": "^1.0.7",
    "react-horizontal-stacked-bar-chart": "^8.15.2",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.2.0",
    "react-split-pane": "^0.1.91",
    "react-table": "^7.1.0",
    "react-tooltip": "^4.2.6",
    "scroll-into-view": "^1.14.2",
    "simplebar-react": "^3.0.0-beta.8",
    "sql-formatter": "^2.3.3",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@grafana/toolkit": "7.1.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/grafana": "github:CorpGlory/types-grafana",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^8.0.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-scrollbar": "^0.5.1",
    "babel-jest": "^24.9.0",
    "codeceptjs": "^2.6.1",
    "codeceptjs-influxdbhelper": "^1.0.0",
    "codeceptjs-resemblehelper": "^1.9.0",
    "codeceptjs-saucehelper": "^1.4.0",
    "codecov": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "identity-obj-proxy": "^3.0.0",
    "influx": "^5.5.2",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "less": "^3.10.3",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi": "^1.1.0",
    "mochawesome": "^4.0.1",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^5.1.0",
    "node-sass": "^4.12.0",
    "playwright": "^0.12.1",
    "sass-loader": "^8.0.2",
    "selenium-standalone": "^6.17.0",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.0",
    "typescript": "^3.9.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.0.3"
  },
  "resolutions": {
    "@babel/preset-env": "7.9.0"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  }
}
