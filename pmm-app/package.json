{
  "name": "pmm-app",
  "version": "2.6.0",
  "description": "PMM UI",
  "author": "Percona",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/percona/grafana-dashboards.git"
  },
  "main": "module.js",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "test:update": "grafana-toolkit plugin:test --updateSnapshot",
    "coverage": "grafana-toolkit plugin:test --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "e2e": "docker run --net=host -v $PWD:/tests codeception/codeceptjs codeceptjs run-multiple parallel --all --debug --steps -c pr.codecept.js --grep '(?=.*)^(?!.*@visual-test)^(?!.*@not-pr-pipeline)'",
    "e2e:single": "codeceptjs run --steps  -c pr.codecept.js",
    "e2e:qan": "docker run --net=host -v $PWD:/tests codeception/codeceptjs codeceptjs run --steps  -c pr.codecept.js --grep '@new-qan' ",
    "codecov": "codecov"
  },
  "bin": {
    "toolkit": "node_modules/grafana/packages/grafana-toolkit/bin/grafana-toolkit.js"
  },
  "keywords": [
    "percona",
    "qan",
    "pmm",
    "grafana"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@grafana/data": "6.7.2",
    "@grafana/runtime": "6.7.2",
    "@grafana/ui": "6.7.2",
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "emotion": "^10.0.27",
    "final-form": "^4.18.5",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "numeral": "^2.0.6",
    "react": "^16.12.0",
    "react-final-form": "^6.3.5",
    "react-highlight.js": "^1.0.7",
    "react-horizontal-stacked-bar-chart": "^8.15.2",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.1.2",
    "react-scrollbar": "^0.5.6",
    "react-split": "^2.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@grafana/toolkit": "6.7.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/lodash": "^4.14.150",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "babel-jest": "^24.9.0",
    "codeceptjs": "^2.6.1",
    "codeceptjs-resemblehelper": "^1.9.0",
    "codeceptjs-saucehelper": "^1.4.0",
    "codecov": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "less": "^3.10.3",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi": "^1.1.0",
    "mochawesome": "^4.0.1",
    "node-sass": "^4.12.0",
    "playwright": "^0.12.1",
    "react-hooks-testing-library": "^0.6.0",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^8.0.2",
    "selenium-standalone": "^6.17.0",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.0",
    "typescript": "^3.6.3",
    "webdriverio": "^5.18.7",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.0.3"
  },
  "resolutions": {
    "@babel/preset-env": "7.9.0"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  }
}
