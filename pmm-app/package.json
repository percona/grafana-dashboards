{
  "name": "pmm-qan-app",
  "version": "1.0.0",
  "description": "PMM app for Grafana",
  "main": "module.js",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test --updateSnapshot",
    "coverage": "grafana-toolkit plugin:test --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "clean:reports": "rm -rf cypress/reports; rm -rf cypress/screenshots",
    "merge:report": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/full_report.json",
    "generate:report": "npx marge cypress/reports/*.json -f report -o cypress/reports/",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/reports",
    "report:copyVideos": "cp -r cypress/videos cypress/reports",
    "after:tests": "npm run merge:report; npm run report:copyScreenshots; npm run report:copyVideos; npm run generate:report",
    "e2e": "docker run -i -v $PWD:/e2e -w /e2e cypress/included:3.4.0 --config baseUrl=http://host.docker.internal",
    "e2e:single": "codeceptjs run --steps  -c pr.codecept.json",
    "codecov": "codecov"
  },
  "bin": {
    "toolkit": "node_modules/grafana/packages/grafana-toolkit/bin/grafana-toolkit.js"
  },
  "keywords": [
    "percona",
    "qan",
    "pmm",
    "grafana"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@grafana/runtime": "^6.5.2",
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "codeceptjs": "^2.5.0",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-multi-reporters": "^1.2.4",
    "emotion": "^10.0.27",
    "final-form": "^4.18.5",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "mocha": "^7.1.1",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^4.1.0",
    "numeral": "^2.0.6",
    "react": "^16.11.0",
    "react-final-form": "^6.3.0",
    "react-json-view": "^1.19.1",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@grafana/data": "6.4",
    "@grafana/toolkit": "6.7.0",
    "@grafana/ui": "6.4",
    "@testing-library/react-hooks": "^1.0.4",
    "@types/grafana": "github:CorpGlory/types-grafana.git",
    "@types/lodash": "^4.14.74",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.9.0",
    "babel-jest": "^24.9.0",
    "codeceptjs": "^2.5.0",
    "codeceptjs-resemblehelper": "^1.9.0",
    "codeceptjs-saucehelper": "^1.4.0",
    "codecov": "^3.6.5",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^3.2.0",
    "cypress": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi": "^1.1.0",
    "mochawesome": "^4.1.0",
    "node-sass": "^4.12.0",
    "numeral": "^2.0.6",
    "react-final-form-hooks": "^2.0.2",
    "react-hooks-testing-library": "^0.6.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.12.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass-loader": "^7.3.1",
    "selenium-standalone": "^6.17.0",
    "sql-formatter": "^2.3.3",
    "style-loader": "^1.0.0",
    "ts-loader": "^4.4.0",
    "typescript": "^3.6.3",
    "webdriverio": "^5.18.7",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.0.3"
  },
  "author": "Percona",
  "license": "AGPL"
}
